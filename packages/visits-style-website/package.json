{
  "name": "visits-style-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mdx-js/loader": "^0.15.5",
    "@mdx-js/mdx": "^0.15.5",
    "@mdx-js/tag": "^0.15.0",
    "animejs": "^2.2.0",
    "codemirror": "^5.40.2",
    "gatsby": "^2.0.28",
    "gatsby-mdx": "^0.2.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.7",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-remark-images": "^2.0.4",
    "gatsby-source-filesystem": "^2.0.4",
    "gatsby-transformer-react-docgen": "^2.1.1",
    "polished": "^1.9.2",
    "prism-react-renderer": "^0.1.4",
    "react": "^16.4.1",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.1",
    "react-live": "^1.12.0",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.0.2"
  },
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "build": "rm -rf ./public && gatsby build",
    "deploy": "rm -rf ./public && gatsby build --prefix-paths && gh-pages -d public",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/codemirror": "^0.0.65",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-helmet": "^5.0.7",
    "@types/react-icons": "^2.2.6",
    "@types/react-transition-group": "^2.0.14",
    "@types/styled-components": "^4.0.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-react-docgen": "^2.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.2",
    "prismjs": "^1.15.0",
    "react-docgen-typescript": "^1.10.0",
    "react-syntax-highlighter": "^8.0.1",
    "ts-jest": "^23.10.3",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1",
    "typescript-styled-plugin": "^0.11.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
