{
  "name": "visits-style",
  "author": "@nukbal",
  "repository": "https://github.com/visits-works/visits-style",
  "version": "1.0.0-alpha.3",
  "private": true,
  "main": "index.js",
  "module": "visits-style.esm.js",
  "jsnext:main": "visits-style.esm.js",
  "typings": "index.d.ts",
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=4.2.0"
  },
  "dependencies": {
    "polished": "^3.4.1",
    "react-transition-group": "^4.1.1"
  },
  "scripts": {
    "build": "tsdx build && cp ./package.json ./dist/package.json",
    "lint": "eslint --ext .tsx,.ts src",
    "test": "jest test --env=jsdom",
    "develop": "cd ../visits-style-website && yarn start",
    "deploy": "yarn build && gh-pages -d dist -b master",
    "clean": "rm -rf ./dist",
    "start": "start-storybook -p 9000",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "yarn storybook:build && gh-pages -d .out -e storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-options": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/react": "^5.1.3",
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-transition-group": "^2.9.1",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-preset-react-app": "^9.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "^3.8.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "faker": "^4.1.0",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.1",
    "tsdx": "^0.7.1",
    "typescript": "^3.5.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
