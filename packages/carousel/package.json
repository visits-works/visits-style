{
  "name": "@visits-style/carousel",
  "repository": "https://github.com/visits-works/visits-style/tree/master/packages/carousel",
  "version": "0.0.1",
  "main": "index.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "typings": "index.d.ts",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=4.2.0"
  },
  "dependencies": {
    "polished": "^3.4.1",
    "react-transition-group": "^4.3.0"
  },
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint src",
    "test": "tsdx test",
    "test:ci": "tsdx test --ci --coverage"
  },
  "devDependencies": {
    "tsdx": "^0.9.3",
    "typescript": "^3.6.3"
  },
  "jest": {
    "testURL": "http://localhost",
    "testMatch": [
      "<rootDir>/**/*.test.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.ts"
    ],
    "rootDir": ".",
    "collectCoverageFrom": [
      "src/**/**.{ts,tsx}",
      "!src/**/**.story.tsx",
      "!src/styles/*.ts",
      "!src/index.ts",
      "!src/elements/index.ts",
      "!src/forms/index.ts",
      "!src/components/index.ts"
    ]
  }
}
