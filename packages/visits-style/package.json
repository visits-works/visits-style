{
  "name": "visits-style",
  "author": "@Nukbal",
  "repository": "https://github.com/visits-works/visits-style",
  "version": "0.0.1",
  "private": true,
  "main": "cjs/index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "typings": "index.d.ts",
  "peerDependencies": {
    "react": ">=16.4.1",
    "react-dom": ">=16.4.1",
    "styled-components": ">=4.1.1"
  },
  "dependencies": {
    "polished": "^2.3.1",
    "react-transition-group": "^2.5.1"
  },
  "scripts": {
    "compile": "tsc -d --emitDeclarationOnly --outDir ./build && tsc --outDir ./build/cjs -m commonjs",
    "babel": "NODE_ENV=production babel ./src --out-dir build --extensions \".ts,.tsx\" --ignore \"src/**/*.test.tsx\",\"src/**/*.story.tsx\",\"**/__test__/**\"",
    "build": "yarn clean && yarn compile && yarn babel && cp package.json ./build/package.json",
    "develop": "cd ../visits-style-website && yarn start",
    "deploy": "yarn build && gh-pages -d build -b master",
    "clean": "rm -rf ./build",
    "test": "jest --env=jsdom",
    "start": "start-storybook -p 9000",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-knobs": "^4.0.12",
    "@storybook/addon-options": "^4.0.12",
    "@storybook/addon-viewport": "^4.0.12",
    "@storybook/components": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "@svgr/rollup": "^4.1.0",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-icons": "^2.2.6",
    "@types/react-syntax-highlighter": "^0.0.6",
    "@types/react-transition-group": "^2.0.14",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__addon-options": "^4.0.0",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.9.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "jest": "^23.6.0",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "typescript-styled-plugin": "^0.11.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
