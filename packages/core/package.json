{
  "name": "visits-style",
  "repository": "https://github.com/visits-works/visits-style",
  "homepage": "https://github.com/visits-works/visits-style",
  "version": "1.1.3",
  "main": "dist/index.js",
  "module": "dist/visits-style.esm.js",
  "jsnext:main": "dist/visits-style.esm.js",
  "typings": "dist/index.d.ts",
  "license": "Apache-2.0",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=4.2.0"
  },
  "dependencies": {
    "polished": "^4.1.3",
    "@popperjs/core": "^2.9.2",
    "react-transition-group": "^4.4.2"
  },
  "scripts": {
    "build": "NODE_ENV=production tsdx build",
    "lint": "tsdx lint src",
    "test": "tsdx test",
    "test:ci": "CI=true tsdx test --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^24.0.17",
    "@types/puppeteer": "^1.19.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-transition-group": "^4.2.3",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^25.5.1",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-react-app": "^9.1.2",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.19.0",
    "faker": "^4.1.0",
    "tsdx": "^0.14.1",
    "typescript": "^4.3.4"
  },
  "jest": {
    "testURL": "http://localhost",
    "testMatch": [
      "<rootDir>/**/*.test.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.ts"
    ],
    "rootDir": ".",
    "collectCoverageFrom": [
      "src/**/**.{ts,tsx}",
      "!src/**/**.story.tsx",
      "!src/styles/*.ts",
      "!src/index.ts",
      "!src/elements/index.ts",
      "!src/forms/index.ts",
      "!src/components/index.ts"
    ]
  }
}
