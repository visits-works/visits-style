{
  "name": "visits-style",
  "version": "0.0.1",
  "private": true,
  "main": "cjs/index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "typing": "index.d.ts",
  "peerDependencies": {
    "react": ">=16.4.1",
    "react-dom": ">=16.4.1",
    "styled-components": ">=3.4.9"
  },
  "dependencies": {
    "polished": "^2.3.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.1.1"
  },
  "scripts": {
    "compile": "tsc -d --outDir ./build --m esnext --t esnext",
    "compile:cjs": "tsc --outDir ./build/cjs --m commonjs",
    "build": "NODE_ENV=production yarn clean && yarn compile && yarn compile:cjs && cp package.json ./build/package.json",
    "develop": "cd ../visits-style-website && yarn start",
    "deploy": "yarn build && gh-pages -d build -b master",
    "clean": "rm -rf ./build",
    "test": "jest --env=jsdom",
    "start": "start-storybook -p 9000",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@storybook/addon-knobs": "^4.0.6",
    "@storybook/addon-options": "^4.0.6",
    "@storybook/addon-viewport": "^4.0.6",
    "@storybook/components": "^4.0.6",
    "@storybook/react": "^4.0.6",
    "@svgr/rollup": "^3.1.0",
    "@types/animejs": "^2.0.0",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "@types/prismjs": "^1.9.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-icons": "^2.2.6",
    "@types/react-syntax-highlighter": "^0.0.6",
    "@types/react-transition-group": "^2.0.14",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__addon-options": "^3.2.2",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^3.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.8.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.1",
    "react-app-polyfill": "^0.1.3",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-url": "^2.0.1",
    "ts-jest": "^23.10.3",
    "ts-loader": "^5.2.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1",
    "typescript-styled-plugin": "^0.11.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
