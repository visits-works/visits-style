  },
  "dependencies": {
    "@floating-ui/react": "^0.24.3",
    "polished": "^4.2.2"
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint 'src/**/*.{ts,tsx}' && tsc --noEmit",
    "test": "vitest",
    "test:ci": "CI=true vitest",
    "build_doc": "yarn storybook:build && yarn workspace visits-style-website build",
