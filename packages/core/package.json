{
  "name": "visits-style",
  "repository": "https://github.com/visits-works/visits-style/tree/master/packages/core",
  "version": "1.0.0-alpha.13",
  "main": "index.js",
  "module": "visits-style.esm.js",
  "jsnext:main": "visits-style.esm.js",
  "typings": "index.d.ts",
  "license": "Apache-2.0",
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=4.2.0"
  },
  "dependencies": {
    "polished": "^3.4.1",
    "react-transition-group": "^4.3.0"
  },
  "scripts": {
    "build": "NODE_ENV=production tsdx build && cp ./package.json ./dist/package.json && cp ../../LICENSE ./dist/LICENSE && cp ./README.md ./dist/README.md",
    "lint": "tsdx lint src",
    "test": "tsdx test",
    "test:ci": "tsdx test --ci --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.17",
    "@types/puppeteer": "^1.19.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-transition-group": "^4.2.3",
    "@types/styled-components": "^4.1.19",
    "babel-jest": "^24.9.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-react-app": "^9.0.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "faker": "^4.1.0",
    "tsdx": "^0.10.5",
    "typescript": "^3.6.3"
  },
  "jest": {
    "testURL": "http://localhost",
    "testMatch": [
      "<rootDir>/**/*.test.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.ts"
    ],
    "rootDir": ".",
    "collectCoverageFrom": [
      "src/**/**.{ts,tsx}",
      "!src/**/**.story.tsx",
      "!src/styles/*.ts",
      "!src/index.ts",
      "!src/elements/index.ts",
      "!src/forms/index.ts",
      "!src/components/index.ts"
    ]
  }
}
