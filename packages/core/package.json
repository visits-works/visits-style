{
  "name": "visits-style",
  "repository": "https://github.com/visits-works/visits-style",
  "homepage": "https://github.com/visits-works/visits-style",
  "version": "1.3.0",
  "main": "dist/index.js",
  "module": "dist/visits-style.esm.js",
  "jsnext:main": "dist/visits-style.esm.js",
  "typings": "dist/index.d.ts",
  "license": "Apache-2.0",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=4.2.0"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.0",
    "polished": "^4.2.2",
    "react-transition-group": "^4.4.5"
  },
  "scripts": {
    "build": "NODE_ENV=production tsdx build",
    "lint": "tsdx lint src",
    "test": "tsdx test",
    "test:ci": "CI=true tsdx test --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^24.0.17",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-transition-group": "^4.4.1",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.5",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-react-app": "^10.0.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "rollup-plugin-typescript2": "0.34.0",
    "resize-observer-polyfill": "1.5.1",
    "faker": "^4.1.0",
    "tsdx": "^0.14.1",
    "typescript": "^4.8.3"
  },
  "jest": {
    "testURL": "http://localhost",
    "testMatch": [
      "<rootDir>/**/*.test.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.ts"
    ],
    "rootDir": ".",
    "collectCoverageFrom": [
      "src/**/**.{ts,tsx}",
      "!src/**/**.story.tsx",
      "!src/styles/*.ts",
      "!src/index.ts",
      "!src/elements/index.ts",
      "!src/forms/index.ts",
      "!src/components/index.ts"
    ]
  }
}
