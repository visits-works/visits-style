{
  "name": "visits-style",
  "author": "@Nukbal",
  "repository": "https://github.com/visits-works/visits-style",
  "version": "0.1.1",
  "private": true,
  "main": "visits.cjs.js",
  "module": "visits.js",
  "jsnext:main": "visits.js",
  "typings": "index.d.ts",
  "peerDependencies": {
    "react": ">=16.4.1",
    "react-dom": ">=16.4.1",
    "styled-components": ">=4.1.1"
  },
  "dependencies": {
    "polished": "^3.2.0",
    "react-transition-group": "^2.9.0"
  },
  "scripts": {
    "compile": "tsc -d --emitDeclarationOnly --outDir ./build",
    "build": "yarn clean && yarn compile && rollup -c && cp package.json ./build/package.json",
    "develop": "cd ../visits-style-website && yarn start",
    "deploy": "yarn build && gh-pages -d build -b master --add",
    "clean": "rm -rf ./build",
    "test": "jest --env=jsdom",
    "start": "start-storybook -p 9000",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "yarn storybook:build && gh-pages -d .out -b gh-pages --add --dest storybook",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/core": "^10.0.10",
    "@storybook/addon-actions": "^5.0.9",
    "@storybook/addon-knobs": "^5.0.9",
    "@storybook/addon-options": "^5.0.9",
    "@storybook/addon-viewport": "^5.0.9",
    "@storybook/components": "^5.0.9",
    "@storybook/react": "^5.0.9",
    "@svgr/rollup": "^4.1.0",
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-icons": "^2.2.6",
    "@types/react-syntax-highlighter": "^10.1.0",
    "@types/react-transition-group": "^2.0.16",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^4.0.1",
    "@types/storybook__addon-options": "^4.0.1",
    "@types/storybook__react": "^4.0.1",
    "@types/styled-components": "^4.1.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "jest": "^24.1.0",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.2.1",
    "react-dom": "^16.6.3",
    "rollup": "^1.2.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-visualizer": "^1.0.2",
    "styled-components": "^4.1.3",
    "tslint": "^5.13.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  }
}
