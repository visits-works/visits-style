{
  "name": "visits-style",
  "author": "@nukbal",
  "repository": "https://github.com/visits-works/visits-style",
  "version": "1.0.0-alpha.9",
  "main": "index.js",
  "module": "visits-style.esm.js",
  "jsnext:main": "visits-style.esm.js",
  "typings": "index.d.ts",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6",
    "styled-components": ">=4.2.0"
  },
  "dependencies": {
    "polished": "^3.4.1",
    "react-transition-group": "^4.2.2"
  },
  "scripts": {
    "build": "tsdx build && cp ./package.json ./dist/package.json && cp ../../LICENSE ./dist/LICENSE && cp ../../README.md ./dist/README.md",
    "lint": "tsdx lint src",
    "test": "tsdx test",
    "test:ci": "tsdx test --ci --coverage",
    "clean": "rm -rf ./dist",
    "start": "start-storybook -p 9000",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "yarn storybook:build && gh-pages -d .out -e storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-options": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.17",
    "@types/puppeteer": "^1.19.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-transition-group": "^4.2.2",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-preset-react-app": "^9.0.1",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "14.0.0",
    "faker": "^4.1.0",
    "gh-pages": "^2.1.1",
    "puppeteer": "^1.19.0",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.2",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "styled-components": "^4.3.2",
    "tsdx": "^0.9.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "testURL": "http://localhost",
    "testMatch": [
      "<rootDir>/**/*.test.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)?$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTest.ts"
    ],
    "rootDir": ".",
    "collectCoverageFrom": [
      "src/**/**.{ts,tsx}",
      "!src/**/**.story.tsx",
      "!src/styles/*.ts",
      "!src/index.ts",
      "!src/elements/index.ts",
      "!src/forms/index.ts",
      "!src/components/index.ts"
    ]
  }
}
