{
  "name": "@visits/style",
  "repository": "https://github.com/visits-works/visits-style",
  "homepage": "https://github.com/visits-works/visits-style",
  "version": "2.0.0",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./lib.css": "./dist/lib.css"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "files": ["dist"],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "tailwindcss": ">=4"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.3",
    "clsx": "^2.1.1"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint 'src/**/*.{ts,tsx}' && tsc --noEmit",
    "test": "vitest",
    "dev": "STORYBOOK=true storybook dev -p 9000",
    "storybook:build": "STORYBOOK=true storybook build -c .storybook -o .out --disable-telemetry",
    "storybook:deploy": "bun storybook:build && gh-pages -d .out"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-vite": "^8.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/vite": "^4.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.11.2",
    "gh-pages": "^6.3.0",
    "happy-dom": "^16.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.5.3",
    "tailwindcss": "^4.0.3",
    "tsup": "^8.3.6",
    "typescript": "^5.7.2",
    "vite": "^6.0.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.4"
  }
}
