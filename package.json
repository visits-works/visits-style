    "resize-observer-polyfill": "^1.5.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-pure": "^0.1.1",
    "storybook": "^7.0.24",
    "styled-components": "5.3.10",
    "typescript": "^5.1.6",
